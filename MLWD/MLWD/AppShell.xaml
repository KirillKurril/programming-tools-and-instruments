<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MLWD.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MLWD"
    Title="MLWD1">

    <FlyoutItem Title="Calculator" Route="MainPage">
        <ShellContent
            Title="Calculator"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage"/>
    </FlyoutItem>
    <FlyoutItem Title="Progress bar" Route="ProgressBarPage">
        <ShellContent
            Title="Progress bar"
            ContentTemplate="{DataTemplate local:ProgressBarPage}"
            Route="ProgressBarPage"/>
    </FlyoutItem>
    <FlyoutItem Title="SQLiteDemo" Route="SQLiteDemo">
        <ShellContent
        Title="SQLiteDemo"
        ContentTemplate="{DataTemplate local:SQLiteDemo}"
        Route="SQLiteDemo"/>
    </FlyoutItem>
    <FlyoutItem Title="Currency Converter" Route="CurrencyConverter">
        <ShellContent
     Title="Currency Converter"
     ContentTemplate="{DataTemplate local:CurrencyConverter}"
     Route="CurrencyConverter"/>
    </FlyoutItem>
</Shell>
